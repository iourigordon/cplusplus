#include <iostream>
#include <string>

using namespace std;

class CpyConstructor
{
public:
    CpyConstructor(string sName):m_Name(sName){;}
    ~CpyConstructor() {;}
    
    CpyConstructor(CpyConstructor& Cpy):m_Name(Cpy.m_Name)
    {
        ;
    }
private:
    string m_Name;
};

int main(int argv, char** argc)
{
    CpyConstructor A("A");
    CpyConstructor B(A);
};
